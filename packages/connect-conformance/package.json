{
  "name": "@connectrpc/connect-conformance",
  "version": "1.4.0",
  "private": true,
  "main": "./dist/cjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "bin": {
    "connectconformance": "bin/connectconformance"
  },
  "scripts": {
    "generate": "buf generate buf.build/connectrpc/conformance:v1.0.2",
    "clean": "rm -rf ./dist/*",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --project tsconfig.json --module commonjs --verbatimModuleSyntax false --moduleResolution node10 --outDir ./dist/cjs --declaration --declarationDir ./dist/cjs",
    "build:esm": "tsc --project tsconfig.json --module ES2015 --moduleResolution node10 --verbatimModuleSyntax false --outDir ./dist/esm --declaration --declarationDir ./dist/esm && echo >./dist/esm/package.json '{\"type\":\"module\"}'"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^1.10.0",
    "@connectrpc/connect": "1.4.0",
    "fflate": "^0.8.1",
    "tar-stream": "^3.1.7",
    "undici": "^5.28.4"
  },
  "devDependencies": {
    "@types/node-forge": "^1.3.9",
    "@types/tar-stream": "^3.1.3",
    "@types/debug": "^4.1.12"
  }
}
